package com.Programs;
class Chain{
	public Chain() {
		this(2);
		System.out.println("This is default constructor");
	}
	public Chain(int a) {
		this(2,3);
		System.out.println("this is 1 parametric constructor ");
		System.out.println("value os a"+a);
	}
	public Chain(int a,int b) {
		this(3,5,6);
		System.out.println("this is 2 parametric constructor ");
		System.out.println("addition of two numbers is"+(a+b));
	}
	public Chain(int a,int b,int c) {
		System.out.println("this is 3 parametric constructor ");
		System.out.println("addition of three numbers is"+(a+b+c));
	}
	
}

public class Program14 {


	public static void main(String[] args) {
		Chain c=new Chain();

	}

}
